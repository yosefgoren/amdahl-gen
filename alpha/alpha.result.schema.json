{
    "$id": "alpha.result.schema",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "required": [
        "config",
        "context",
        "data"
    ],
    "properties": {
        "config": {
            "$ref": "alpha.config.schema"
        },
        "context": {
            "$ref": "context.schema"
        },
        "data": {
            "type": "object",
            "patternProperties": {
                "^[0-9]+$": {
                    "description": "the alpha value of this line",
                    "type": "number"
                }
            },
            "additionalProperties": false
        }
    }
}