{
    "$id": "significant.result.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "required": [
        "config",
        "context",
        "data"
    ],
    "type": "object",
    "properties": {
        "config": {
            "$ref": "significant.config.schema.json"
        },
        "context": {
            "$ref": "context.schema"
        },
        "data": {
            "type": "object",
            "patternProperties": {
                "^[0-9]+$": {
                    "description": "the average runtime of this line",
                    "type": "number"
                }
            },
            "additionalProperties": false
        }
    }
}