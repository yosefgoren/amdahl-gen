{
    "$id": "significant.result.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "required": [
        "config",
        "context",
        "data"
    ],
    "type": "object",
    "properties": {
        "config": {
            "$ref": "file:significant/significant.config.schema.json"
        },
        "context": {
            "$ref": "file:framework/context.schema.json"
        },
        "data": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "lineno": {
                        "type": "number"
                    },
                    "avg_ipc": {
                        "type": "number"
                    }
                }
            },
            "additionalProperties": false
        }
    }
}