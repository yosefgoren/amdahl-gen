{
    "$id": "alpha.config.schema",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "conf_type": {
            "type": "string",
            "enum": [
                "alpha"
            ]
        },
        "exe_path": {
            "type": "string"
        },
        "thread_counts": {
            "type": "array",
            "items": {
                "type": "number"
            }
        },
        "num_reps": {
            "type": "number"
        }
    }
}